ookie\";\r\nimport {CART_ADD_ITEM, CART_REMOVE_ITEM} from '../constants/cartConstants';\r\n\r\nconst addToCart = (productId, qty) => async (dispatch, getState) => {\r\n\r\n    try {\r\n        const {data} = await Axios.get(\"/api/products/\" + productId);\r\n        dispatch({\r\n        type: CART_ADD_ITEM, payload:{\r\n        product: data._id,\r\n        name: data.name,\r\n        image: data.image,\r\n        price: data.price,\r\n        countInStock: data.countInStock,\r\n        qty\r\n        }});\r\n\r\n        //Inserisco i cookie per salavare i prodotti\r\n        const {cart:{cartItems}} = getState();\r\n        Cookie.set(\"cartItems\", JSON.stringify(cartItems));\r\n\r\n    } catch (error) {\r\n\r\n    }\r\n    const removeFromCartHandler = (productId) => (dispatch, getState) => {\r\n        dispatch({ type: CART_REMOVE_ITEM, payload: productId });\r\n\r\n        const {cart:{cartItems}} = getState();\r\n        Cookie.set(\"cartItems\", JSON.stringify(cartItems));\r\n    }\r\n}\r\nexport {addToCart, /*removeFromCartHandler*/}","C:\\Users\\bemar\\Desktop\\centroCommercialeInCentro\\C3-ATeam\\frontend\\src\\constants\\cartConstants.js",[],"C:\\Users\\bemar\\Desktop\\centroCommercialeInCentro\\C3-ATeam\\frontend\\src\\constants\\productConstants.js",[],"C:\\Users\\bemar\\Desktop\\centroCommercialeInCentro\\C3-ATeam\\frontend\\src\\data.js",["142"],"export default {\r\n    products: [\r\n        {\r\n        _id: '1',\r\n        name: 'Maglietta nera palma',\r\n        category: 'clothing',\r\n        image: './images/a1.jpg',\r\n        price: 50,\r\n        brand: 'Mike',\r\n        rating: 4.5,\r\n        numReiews: 10,\r\n        countInStock: 10,\r\n    },\r\n    {\r\n        _id: '2',\r\n        name: 'Maglietta nera rondine',\r\n        image: './images/a2.jpg',\r\n        price: 40,\r\n        brand: 'Abibas',\r\n        rating: 4.0,\r\n        numReiews: 13,\r\n        countInStock: 10,\r\n    },\r\n    {\r\n        _id: '3',\r\n        name: 