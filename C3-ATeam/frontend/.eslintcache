option>\r\n                                    </select>\r\n                                    <button type=\"button\" className=\"button\" onClick={() => removeFromCartHandler(item.product)}>\r\n                                        Rimuovi dai prodotti selezionati\r\n                                    </button>\r\n                                </div>\r\n                            </div>\r\n                            <div className=\"cart-price\">\r\n                                $ {item.price}\r\n                            </div>\r\n                        </div>\r\n                        )\r\n                }\r\n\r\n            </ul>\r\n\r\n        </div>\r\n        <div className=\"cart-action\">\r\n            <h3>\r\n                Subtotal ( {cartItems.reduce((a, c) => a + c.qty, 0)} itemes)\r\n                :\r\n                 $ {cartItems.reduce((a, c) => a + c.price * c.qty, 0)}\r\n            </h3>\r\n            <button onClick={checkoutHandler} className=\"button primary full-width\" disabled={cartItems.length === 0}>\r\n                Procedi al tuo negozo di fiducia!\r\n            </button>\r\n\r\n        </div>\r\n    </div>\r\n}\r\n\r\nexport default CartScreen;","C:\\Users\\bemar\\Desktop\\centroCommercialeInCentro\\C3-ATeam\\frontend\\src\\reducers\\productReducers.js",[],"C:\\Users\\bemar\\Desktop\\centroCommercialeInCentro\\C3-ATeam\\frontend\\src\\reducers\\cartReducers.js",[],"C:\\Users\\bemar\\Desktop\\centroCommercialeInCentro\\C3-ATeam\\frontend\\src\\actions\\cartActions.js",["163"],"import Axios from \"axios\";\r\nimport Cookie from \"js-cookie\";\r\nimport {CART_ADD_ITEM, CART_REMOVE_ITEM} from '../constants/cartConstants';\r\n\r\nconst addToCart = (productId, qty) => async (dispatch, getState) => {\r\n\r\n    try {\r\n        const {data} = await Axios.get(\"/api/products/\" + productId);\r\n        dispatch({\r\n        type: CART_ADD_ITEM, payload:{\r\n        product: data._id,\r\n        name: data.name,\r\n     